# TrustCoin Bot Environment Variables
# Copy this file to .env and fill in your actual bot tokens

# Bot Tokens from @BotFather
BOT_TOKEN_ENG=your_bot_token_here

PORT=8443
WEBHOOK_URL=
# Leave WEBHOOK_URL empty for local testing

# Debug Mode (True/False)
DEBUG=False

# App Environment
APP_ENV=production

# Logging Level
LOG_LEVEL=INFO

# Admin Configuration (comma-separated user IDs)
ADMIN_USER_IDS=your_user_id_here

# Group Chat IDs for auto-posting (comma-separated)
GROUP_CHAT_IDS=your_group_chat_id_here

# Auto-posting interval in seconds (default: 120 = 2 minutes)
AUTO_POST_INTERVAL=120

# Optional: Database Configuration
# DATABASE_URL=
